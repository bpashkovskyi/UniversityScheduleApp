@model IEnumerable<UniversityScheduleApp.Models.Teacher>

<h1>Teachers</h1>

<table class="table">
    <thead>
        <tr>
            <th>Teacher ID</th>
            <th>Teacher Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var teacher in Model)
        {
            <tr>
                <td>@teacher.Id</td>
                <td>@teacher.Name</td>
            </tr>
        }
    </tbody>
</table>